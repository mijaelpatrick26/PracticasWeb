Algoritmo Ejemplo1
	Definir n1,n2,sum,res,mul,div Como Real;
	n1=0;n2=0;sum=0;res=0;mul=0;div=0;
	Escribir "Ingrese el número 1:";
	leer n1;
	Escribir "Ingrese el numero 2:";
	leer n2;
	sum=n1+n2;
	res=n1-n2;
	mul=n1*n2;
	div=n1/n2;
	Escribir "La suma es: ",sum;
	Escribir "La resta es: ",res;
	Escribir "La multiplicación es: ",mul;
	Escribir "La division es: ",div;
	
	
FinAlgoritmo
